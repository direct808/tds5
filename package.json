  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --config ./jest.config.unit.ts",
    "test:watch": "jest --config ./jest.config.unit.ts --watch",
    "test:cov": "jest --config ./jest.config.unit.ts --coverage",
    "test:cov:watch": "npm run test:cov -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./test/jest-e2e.json",
    "kysely-codegen": "kysely-codegen --out-file ./src/shared/db.d.ts",
    "download-ip2-location-database": "nest start --entryFile cli.js -- download-ip2-location-database",
    "prisma:create-full-migraion": "prisma migrate diff --from-empty --to-schema prisma/schema.prisma --script > prisma/migrations/0_init/migration.sql",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:format-check": "cross-env DATABASE_URL=postgres://fake prisma format --check",
    "prisma:db:push": "prisma db push --accept-data-loss",
    "cicd:lint": "yarn run lint && yarn run prisma:format-check",
    "cicd:test": "yarn run prisma generate && yarn run test:cov"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",

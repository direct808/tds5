{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testTimeout": 30000,
  "globalSetup": "<rootDir>/jest.global-setup.ts",
  "setupFilesAfterEnv": ["<rootDir>/jest.setup-files.ts"]
}
